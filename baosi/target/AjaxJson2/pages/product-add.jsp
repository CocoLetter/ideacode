                <div class="box-body">

                    <!--tab页-->
                    <div class="nav-tabs-custom">

                        <!--tab内容-->
                        <div class="tab-content">

                            <!--表单内容-->
                            <div class="tab-pane active" id="tab-form">
                                <div class="row data-type">

                                    <div class="col-md-2 title">语言</div>
                                    <div class="col-md-10 data">
                                        <label for="languages"></label><select class="form-control" id="languages" name="languages">
                                            <option value="0" selected="selected">中文</option>
                                            <option value="1">English</option>
                                        </select>
                                    </div>

                                    <div class="col-md-2 title">产品名称</div>
                                    <div class="col-md-10 data">
                                        <label>
                                            <input type="text" class="form-control" name="productName"
                                                   value="">
                                        </label>
                                    </div>

                                    <div class="col-md-2 title">产品价格</div>
                                    <div class="col-md-10 data">
                                        <label>
                                            <input type="text" class="form-control" name="productPrice"
                                                   value="">
                                        </label>
                                    </div>

                                    <div class="col-md-2 title" style="height: 200px">产品介绍</div>
                                    <div class="col-md-10 data" style="height: 200px">
                                        <script id="containerD" type="text/plain" name="productDesc"></script>
                                    </div>

                                    <div class="col-md-2 title editer">产品特点</div>
                                    <div class="col-md-10 data editer">
                                        <script id="containerC" type="text/plain" name="productChar"></script>
                                    </div>

                                    <div class="col-md-2 title">产品图片</div>
                                    <div class="col-md-10 data">
                                        <input type="file" name="pictureFile" id="pictureFile" value="请选择图片"/>
                                    </div>

                                    <div class="col-md-2 title">产品参数表</div>
                                    <div class="col-md-10 data">
                                        <input type="file" name="prodParamFile" id="prodParamFile" value="请选择图片"/>
                                    </div>

                                    <div class="col-md-2 title"></div>
                                    <div class="col-md-10 data text-center">
                                        <button type="submit" class="btn bg-maroon">保存</button>
                                        <button type="button" class="btn bg-default" onclick="history.back(-1);">返回
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <!--表单内容/-->

                        </div>
                        <!--tab内容/-->

                    </div>
                    <!--tab页/-->


                    <!-- .box-footer
        <div class="box-footer"></div>
        -->
